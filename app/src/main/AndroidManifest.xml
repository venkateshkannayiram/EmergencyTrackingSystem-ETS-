        <activity
            android:name=".CustomerMapActivity"
            android:label="@string/title_activity_customer_map"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

